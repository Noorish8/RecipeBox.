<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg">
    <View
        android:id="@+id/img_back_arrow"
        android:layout_width="21dp"
        android:layout_height="11dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="37dp"
        android:background="@drawable/ic_back_arround"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/txt_about_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/about_us"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/img_logo"
        app:layout_constraintTop_toTopOf="@+id/img_back_arrow"
        app:layout_constraintEnd_toEndOf="@+id/img_logo"
        android:textColor="@color/dark_black"
        android:textSize="16sp"/>


    <ImageView
        android:id="@+id/img_logo"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_marginTop="27dp"
        android:src="@drawable/about_us_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_about_us"/>

    <TextView
        android:id="@+id/txt_cook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cookry"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/img_logo"
        app:layout_constraintTop_toBottomOf="@+id/img_logo"
        app:layout_constraintEnd_toEndOf="@+id/img_logo"
        android:textColor="@color/dark_green"/>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/txt_virsion_num"
        app:layout_constraintTop_toTopOf="@+id/txt_virsion" />

    <TextView
        android:id="@+id/txt_virsion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/txt_cook"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view"
        android:textSize="14dp"
        android:text="Virsion"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:textColor="@color/white"
        android:drawableStart="@drawable/ic_virsion"
        android:layout_marginTop="20dp"/>
    <TextView
        android:id="@+id/txt_virsion_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/txt_virsion"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/txt_virsion"
        android:text="1.0.0"/>


    <View
        android:id="@+id/view_company"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toTopOf="@+id/txt_company"
        app:layout_constraintBottom_toBottomOf="@+id/txt_company_num"/>
    <TextView
        android:id="@+id/txt_company"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view"
        android:textSize="14dp"
        android:text="Company"
        android:textColor="@color/white"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:drawableStart="@drawable/ic_company"
        android:layout_marginTop="20dp"/>
    <TextView
        android:id="@+id/txt_company_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_company"
        app:layout_constraintTop_toBottomOf="@+id/txt_company"
        app:layout_constraintStart_toStartOf="@+id/view_company"
        app:layout_constraintEnd_toEndOf="@+id/txt_company"
        android:text="ABCD"/>

    <View
        android:id="@+id/view_email"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toTopOf="@+id/txt_email"
        app:layout_constraintBottom_toBottomOf="@+id/txt_email_num"/>

    <TextView
        android:id="@+id/txt_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/view_company"
        app:layout_constraintStart_toStartOf="@+id/view_email"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view_email"
        android:textSize="14dp"
        android:text="Email"
        android:textColor="@color/white"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:drawableStart="@drawable/ic_email_abt"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txt_email_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_email"
        app:layout_constraintTop_toBottomOf="@+id/txt_email"
        app:layout_constraintStart_toStartOf="@+id/view_email"
        app:layout_constraintEnd_toEndOf="@+id/txt_email"
        android:text="abcd@gmail.vcom"/>

    <View
        android:id="@+id/view_website"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toTopOf="@+id/txt_website"
        app:layout_constraintBottom_toBottomOf="@+id/txt_website_num"/>

    <TextView
        android:id="@+id/txt_website"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/view_email"
        app:layout_constraintStart_toStartOf="@+id/view_email"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view_email"
        android:textSize="14dp"
        android:text="Website"
        android:textColor="@color/white"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:drawableStart="@drawable/ic_web_img"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txt_website_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_website"
        app:layout_constraintTop_toBottomOf="@+id/txt_website"
        app:layout_constraintStart_toStartOf="@+id/txt_website"
        app:layout_constraintEnd_toEndOf="@+id/txt_website"
        android:text="abcd@gmail.vcom"/>

    <View
        android:id="@+id/view_contact"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toTopOf="@+id/txt_contact"
        app:layout_constraintBottom_toBottomOf="@+id/txt_contact_num"/>

    <TextView
        android:id="@+id/txt_contact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/view_website"
        app:layout_constraintStart_toStartOf="@+id/view_contact"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view_contact"
        android:textSize="14dp"
        android:text="Contact"
        android:textColor="@color/white"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:drawableStart="@drawable/ic_call_abt"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txt_contact_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_contact"
        app:layout_constraintStart_toStartOf="@+id/txt_contact"
        app:layout_constraintEnd_toEndOf="@+id/txt_contact"
        android:text="895688566"/>

    <View
        android:id="@+id/view_about"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@drawable/custom_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toTopOf="@+id/txt_about"
        app:layout_constraintBottom_toBottomOf="@+id/txt_about_num"/>


    <TextView
        android:id="@+id/txt_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/view_contact"
        app:layout_constraintStart_toStartOf="@+id/view_contact"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="@+id/view_contact"
        android:textSize="14dp"
        android:text="About"
        android:textColor="@color/white"
        android:drawablePadding="13dp"
        android:paddingStart="13dp"
        android:paddingTop="13dp"
        android:drawableStart="@drawable/ic_virsion"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/txt_about_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constrainedWidth="true"
        android:textSize="14dp"
        app:layout_constrainedHeight="true"
        android:text="@string/lorem_ipsum_dolor_sit_amet_consectetur_adipiscing_elit_etiam_eu_turpis_molestie_dictum_est_a_mattis_tellus_sed_dignissim_metus_nec_fringilla_accumsan_risus_sem_sollicitudin_lacus_ut_interdum_tellus"
        android:textColor="@color/light_gray"
        android:paddingBottom="11dp"
        android:layout_marginStart="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_about"
        app:layout_constraintTop_toBottomOf="@+id/txt_about"
        app:layout_constraintStart_toStartOf="@+id/txt_about"
        app:layout_constraintEnd_toEndOf="@+id/txt_about"
        />












